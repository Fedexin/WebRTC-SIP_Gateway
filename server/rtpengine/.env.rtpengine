# RTPEngine Configuration

# Cloud provider (leave empty for local)
RTPENGINE_CLOUD=

# Public IP addresses
RTPENGINE_PUBLIC_IP=192.168.1.127

# NG control protocol binding
RTPENGINE_BIND_NG_IP=0.0.0.0
RTPENGINE_BIND_NG_PORT=22222

# HTTP management interface
RTPENGINE_BIND_HTTP_IP=0.0.0.0
RTPENGINE_BIND_HTTP_PORT=9090

# RTP media port range
RTPENGINE_PORT_MIN=10000
RTPENGINE_PORT_MAX=20000

# Logging (0=EMERG, 1=ALERT, 2=CRIT, 3=ERR, 4=WARN, 5=NOTICE, 6=INFO, 7=DEBUG)
RTPENGINE_LOG_LEVEL=6

# Recording
RTPENGINE_RECORDING_ENABLED=false
RTPENGINE_RECORDING_METHOD=pcap
RTPENGINE_RECORDING_FORMAT=eth

# HOMER integration (optional)
RTPENGINE_HOMER_ADDR=
RTPENGINE_HOMER_PROTOCOL=udp
RTPENGINE_HOMER_ID=
